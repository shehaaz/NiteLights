            android:name="com.android.nitelights.ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.nitelights.ui.CollectionDemoActivity"
            android:label="@string/title_activity_collection_demo" >
        </activity>
        <activity
            android:name="com.android.nitelights.ui.VenuesFragment$VenuesActivity"
            android:label="@string/title_venues">"
